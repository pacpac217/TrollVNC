<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>TrollVNC</string>
	<key>items</key>
	<array>

		<!-- Managed Mode Notice -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string></string>
			<key>footerText</key>
			<string>This TrollVNC instance is managed by your organization. You can still configure settings below.</string>
		</dict>

		<!-- Reverse Connection -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Reverse Connection</string>
			<key>footerText</key>
			<string>Enter server IP and port. Device ID will be auto-generated from port number. Examples: serverapi.xyz:10010 (Device-10), serverapi.xyz:10300 (Device-300). Port format: 10001-10500 (Device 1-500).</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>defaults</key>
			<string>com.82flex.trollvnc</string>
			<key>key</key>
			<string>ReverseSocket</string>
			<key>label</key>
			<string>Server (IP:Port)</string>
			<key>placeholder</key>
			<string>serverapi.xyz:10010</string>
			<key>noAutoCorrect</key>
			<true/>
			<key>keyboardType</key>
			<string>URL</string>
			<key>footerText</key>
			<string>Port determines device number: 10010 = Device-10, 10300 = Device-300. Only enter IP:Port, Device ID will be auto-generated.</string>
		</dict>

	</array>
</dict>
</plist>
